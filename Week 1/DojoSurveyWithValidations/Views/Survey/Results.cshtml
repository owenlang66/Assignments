@{
    ViewData["Title"] = "Home Page";
}
@model SurveyModel

<div>
    <h1>Here's your Results!</h1>
    @* Model is used to pass data from controllers to views *@
    <p>Name: @Model.Name</p>
    <p>Location: @Model.Location</p>
    <p>Language: @Model.Language</p>
    <p>Comments: @Model.Comments</p>
    <a href="/">Take me back!</a>
</div>
